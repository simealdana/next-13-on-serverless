  "name": "notarizer-front",
  "private": true,
  "scripts": {
    "build": "tailwindcss ./styles/globals.css --output ./styles/output.css && next build",
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./styles/globals.css --output ./styles/output.css --watch\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "lint:css": "stylelint './components/***/**/*.styles.ts'",
    "lint:ts": "next lint",
    "start": "next start",
    "test:e2e": "playwright test",
    "test:unit:watch": "jest --watch",
    "test:unit": "jest",
    "deploy:dev": "./bash-deploy-dev",
    "deploy:uat": "./bash-deploy-uat"
  },
  "version": "0.1.0"
